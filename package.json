  "bugs": {
    "url": "https://github.com/aleen42/geometries/issues"
  },
  "homepage": "https://github.com/aleen42/geometries#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-config-coremail": "^0.1.1",
    "eslint-loader": "^1.7.1",
